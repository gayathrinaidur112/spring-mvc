package com.firstJava;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;  
import org.openqa.selenium.ie.InternetExplorerDriver;  


public class SeleniumTestCases {
	 public static void main(String[] args) {  
	        
	        // System Property for IEDriver   
	    System.setProperty("webdriver.ie.driver", "C:\\Users\\gayathriemuru\\Downloads\\IEDriverServer_x64_2.48.0\\IEDriverServer.exe");  
	      
	       // Instantiate a IEDriver class.       
	    WebDriver driver=new InternetExplorerDriver();  
	      
	       // Launch Website  
	    driver.navigate().to("http://localhost:8080/FirstJavaProgramInSpringMVC/");  
	      
	       //Maximize the browser  
	      driver.manage().window().maximize();  
	        
	       
	        

	}  

}
